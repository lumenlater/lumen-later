{
  "name": "@lumenlater/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "pnpm --filter @lumenlater/shared build && pnpm prisma:generate:all && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:generate:pg": "prisma generate --schema=prisma/schema.postgres.prisma",
    "prisma:generate:all": "pnpm prisma:generate && pnpm prisma:generate:pg",
    "prisma:push": "prisma db push",
    "prisma:push:pg": "prisma db push --schema=prisma/schema.postgres.prisma",
    "prisma:studio": "prisma studio",
    "prisma:studio:pg": "prisma studio --schema=prisma/schema.postgres.prisma",
    "initialize": "tsx scripts/initialize-contracts.ts",
    "seed:merchants": "tsx scripts/seed-merchant.ts",
    "test-merchant-status": "tsx src/scripts/test-merchant-status.ts",
    "test-merchant-status-simple": "tsx src/scripts/test-merchant-status-simple.ts"
  },
  "dependencies": {
    "@creit.tech/stellar-wallets-kit": "^1.7.7",
    "@lumenlater/bnpl-core-client": "workspace:*",
    "@lumenlater/lp-token-client": "workspace:*",
    "@lumenlater/shared": "workspace:*",
    "@lumenlater/usdc-token-client": "workspace:*",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@stellar/stellar-sdk": "14.0.0-rc.3",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.534.0",
    "motion": "^12.23.12",
    "next": "15.4.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@radix-ui/react-select": "^2.2.5",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.17",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "prisma": "^6.13.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3"
  }
}
